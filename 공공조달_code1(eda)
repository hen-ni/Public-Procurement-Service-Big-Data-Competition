= 49: \n","      tag_count.append(dics) \n","      tags.append(dics['tag'])\n","  tag_count20_list[i]= tag_count \n","\n","  df20_mi_final = df20_mi_final.append(tag_count20_list[i]) "],"metadata":{"id":"z3r48-UhQzcZ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["df20_mi_final.to_csv('//content/drive/Shareddrives/공공조달/data/df20_mi_final.csv',index=False)"],"metadata":{"id":"9Etv49HrRTiQ"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["### 단어의 빈도수 확인 - 고등학교"],"metadata":{"id":"CcGVluiVQZfZ"}},{"cell_type":"code","source":["df20_hi = df20[df20['수요기관명'].str.contains('고등학교', na=False)&df20['업무구분'].str.contains('물품', na = False)]\n","df20_hi"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":745},"id":"92v7UB31oUOn","executionInfo":{"status":"ok","timestamp":1645710105693,"user_tz":-540,"elapsed":21,"user":{"displayName":"김혜인","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"17746326578576065817"}},"outputId":"76da49c1-0eee-4226-b87a-c171faa892cd"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/html":["\n","  <div id=\"df-53c29851-3ff0-44ab-bc64-9f8bfae5470b\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>업무구분</th>\n","      <th>공고명</th>\n","      <th>공고기관명</th>\n","      <th>수요기관명</th>\n","      <th>추정가격(배정금액)</th>\n","      <th>year</th>\n","      <th>month</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>162</th>\n","      <td>물품</td>\n","      <td>문산제일고 특성화과 실험실습기자재 구매</td>\n","      <td>경